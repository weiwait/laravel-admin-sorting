{
    "name": "weiwait-laravel-admin-ext/sorting",
    "description": "this is a multifunctional laravel-admin-ext",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "sortable", "sort"],
    "homepage": "https://github.com/weiwait-laravel-admin-ext/sorting",
    "license": "MIT",
    "authors": [
        {
            "name": "weiwait",
            "email": "mail@weiwait.cn"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "^1.7",
        "spatie/eloquent-sortable": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Weiwait\\Sorting\\": "src/"
        }
    },
    "extra": {
